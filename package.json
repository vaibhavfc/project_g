{
  "name": "glacier",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "main": "lib/components",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@date-io/date-fns": "1.3.13",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.3.10",
    "@types/react-otp-input": "^2.0.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.19.0",
    "global": "^4.4.0",
    "lodash": "^4.17.21",
    "node-sass": "^5.0.0",
    "react-otp-input": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "tsc --build \"./tsconfig.json\"",
    "clean": "rm -rf ./lib",
    "copy:scss": "cd src && find ./ -name '*.scss' | cpio -pdm ../lib && cd ..",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test --coverage --watchAll  --collectCoverageFrom=!src/**/*.stories.tsx",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -c .storybook watch-css -s ./src  --no-manager-cache",
    "build-storybook": "build-storybook -s public",
    "sassdoc": "sassdoc src/sass",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx,.json",
    "fix": "eslint src --fix --ext .js,.jsx,.ts,.tsx,.json",
    "prebuild": "npm run clean",
    "prepublish": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "classnames": "^2.2.6",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.7",
    "@storybook/addon-essentials": "^6.2.7",
    "@storybook/addon-links": "^6.2.7",
    "@storybook/node-logger": "^6.2.7",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.7",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.1.10",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-test-renderer": "^17.0.1",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-test-renderer": "^17.0.2",
    "sassdoc": "^2.7.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  }
}